<UserControl
    x:Class="SNC.OptiRamp.Application.Developer.Extensions.FileManagerExtension.ExtensionControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:v="clr-namespace:SNC.OptiRamp.Application.Developer.Extensions.FileManagerExtension"
    d:DesignHeight="300"
    d:DesignWidth="300"
    mc:Ignorable="d">
    <UserControl.Resources>
        <v:ExtensionControlView x:Key="view" />
    </UserControl.Resources>
    <GroupBox
        Name="LayoutRoot"
        DataContext="{Binding
            Source={StaticResource view}}"
        Header="Manage Files" />
</UserControl>