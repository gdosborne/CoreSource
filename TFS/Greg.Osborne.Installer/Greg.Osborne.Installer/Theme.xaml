<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Greg.Osborne.Installer"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <SolidColorBrush x:Key="InstallerTextBrush" Color="#FF926FCB" />
    <SolidColorBrush x:Key="SeparatorLineBrush" Color="#FFF0F0F0"/>
    <SolidColorBrush x:Key="TabBottomBrush" Color="#FF33CD48"/>
    <SolidColorBrush x:Key="StandardTextBrush" Color="#FF474747" />
    <SolidColorBrush x:Key="WindowBackgroundBrush" Color="#FFFFFFFF" />
    <SolidColorBrush x:Key="ItemHighlightBrush" Color="#FFE8E8E8" />
    <SolidColorBrush x:Key="ItemHighlightTextBrush" Color="#FFFF0000" />

    <system:Double x:Key="InstructionAreaWidth">350</system:Double>
    <system:Double x:Key="StandardFontSize">11.0</system:Double>
    <system:Double x:Key="TabFontSize">16.0</system:Double>
    <system:Double x:Key="HeaderFontSize">36.0</system:Double>
    <system:Double x:Key="InstructionMinorFontSize">24.0</system:Double>
    <system:Double x:Key="TooltipTextFontSize">10.0</system:Double>
</ResourceDictionary>