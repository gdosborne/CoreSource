<Window x:Class="Life.Savings.SettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Life.Savings"
        xmlns:conv="clr-namespace:Life.Savings.Converters"
        mc:Ignorable="d"
        Title="Settings" Height="350" Width="850" 
        ShowInTaskbar="False" ResizeMode="NoResize" Loaded="Window_Loaded">
    <Window.DataContext>
        <local:SettingsWindowView PropertyChanged="View_PropertyChanged" ShowBrowseFolder="SettingsWindowView_ShowBrowseFolder" />
    </Window.DataContext>
    <Window.Resources>
        <conv:TelephoneConverter x:Key="TelephoneConverter" />
    </Window.Resources>
    <Grid Name="LayoutRoot">
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition />
        </Grid.RowDefinitions>
        <Border Name="PrimaryBorder" Grid.Row="1" Style="{StaticResource GlobalBorder}">
            <Grid Name="PrimaryGrid">
                <Grid.RowDefinitions>
                    <RowDefinition />
                    <RowDefinition Height="auto" />
                </Grid.RowDefinitions>
                <TabControl Grid.Row="0" Name="TabContoller" Margin="5" BorderThickness="1,1,1,1.5">
                    <TabItem>
                        <TabItem.Header>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="&#xE12A;" FontFamily="Segoe MDL2 Assets" FontSize="12" VerticalAlignment="Center" Margin="0,0,5,0" />
                                <TextBlock Text="Application" VerticalAlignment="Center" />
                            </StackPanel>
                        </TabItem.Header>
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                            </Grid.RowDefinitions>
                            <Grid Grid.Row="0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="auto" />
                                    <ColumnDefinition />
                                    <ColumnDefinition Width="auto" />
                                </Grid.ColumnDefinitions>                                
                                <Label Grid.Column="0" Content="Static data location:" />
                                <TextBox Name="RepoLocationTextBox" Text="{Binding RepositoryLocation,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}" Grid.Column="1" VerticalAlignment="Center" IsReadOnly="True" GotFocus="RepoLocationTextBox_GotFocus" />
                                <Button Grid.Column="2" Width="{Binding ElementName=RepoLocationTextBox,Path=ActualHeight}" Height="{Binding ElementName=RepoLocationTextBox,Path=ActualHeight}" Command="{Binding SelectRepoFolderCommand}">
                                    <TextBlock Text="&#xEC25;" Style="{StaticResource IconTB}" FontSize="12" />
                                </Button>                                
                            </Grid>
                            <Grid Grid.Row="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="auto" />
                                    <ColumnDefinition />
                                    <ColumnDefinition Width="auto" />
                                </Grid.ColumnDefinitions>
                                <Label Grid.Column="0" Content="Illustration save location:" />
                                <TextBox Text="{Binding XpsFileSaveLocation,UpdateSourceTrigger=LostFocus,Mode=TwoWay}" Grid.Column="1" VerticalAlignment="Center" IsReadOnly="True" GotFocus="RepoLocationTextBox_GotFocus" />
                                <Button Grid.Column="2" Width="{Binding ElementName=RepoLocationTextBox,Path=ActualHeight}" Height="{Binding ElementName=RepoLocationTextBox,Path=ActualHeight}" Command="{Binding SelectXpsFolderCommand}">
                                    <TextBlock Text="&#xEC25;" Style="{StaticResource IconTB}" FontSize="12" />
                                </Button>
                            </Grid>
                            <Grid Grid.Row="2">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="auto" />
                                    <ColumnDefinition />
                                </Grid.ColumnDefinitions>
                                <Label Grid.Column="0" Content="Insurance Company Name:" />
                                <TextBox Text="{Binding InsuranceCompanyName,UpdateSourceTrigger=LostFocus,Mode=TwoWay}" Grid.Column="1" VerticalAlignment="Center" GotFocus="RepoLocationTextBox_GotFocus" />
                            </Grid>
                            <StackPanel Grid.Row="3" Orientation="Horizontal">
                                <Label Content="Default State:" />
                                <ComboBox Grid.Column="7" Width="100" HorizontalAlignment="Left" ItemsSource="{Binding States}" SelectedItem="{Binding SelectedState,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}">
                                    <ComboBox.ItemTemplate>
                                        <DataTemplate>
                                            <StackPanel Orientation="Horizontal">
                                                <TextBlock Text="{Binding Abbreviation}" />
                                                <TextBlock Text=" (" />
                                                <TextBlock Text="{Binding Name}" />
                                                <TextBlock Text=")" />
                                            </StackPanel>
                                        </DataTemplate>
                                    </ComboBox.ItemTemplate>
                                </ComboBox>
                            </StackPanel>

                            <CheckBox Grid.Row="4" Content="Encrypt Client Data" Margin="5,2" IsChecked="{Binding IsClientDataEncrypted}" VerticalContentAlignment="Center" />
                            <CheckBox Grid.Row="5" Content="Ask about refresh of static data" Margin="5,2" IsChecked="{Binding IsDoNotAskAboutRefreshChecked}" VerticalContentAlignment="Center" />
                            <CheckBox Grid.Row="6" Content="Ask about clearing form data" Margin="5,2" IsChecked="{Binding IsDoNotAskAboutClearChecked}" VerticalContentAlignment="Center" />
                        </Grid>
                    </TabItem>
                    <TabItem>
                        <TabItem.Header>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="&#xE13D;" FontFamily="Segoe MDL2 Assets" FontSize="12" VerticalAlignment="Center" Margin="0,0,5,0" />
                                <TextBlock Text="Preparer" VerticalAlignment="Center" />
                            </StackPanel>
                        </TabItem.Header>
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                            </Grid.RowDefinitions>
                            <Grid Grid.Row="0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="auto" />
                                    <ColumnDefinition />
                                </Grid.ColumnDefinitions>
                                <Label Grid.Column="0" Content="Name:" />
                                <TextBox Text="{Binding PreparersName,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}" Grid.Column="1" VerticalAlignment="Center" GotFocus="RepoLocationTextBox_GotFocus" />
                            </Grid>
                            <Grid Grid.Row="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="auto" />
                                    <ColumnDefinition />
                                </Grid.ColumnDefinitions>
                                <Label Grid.Column="0" Content="Phone number:" />
                                <TextBox Text="{Binding PreparersPhoneNumber,UpdateSourceTrigger=LostFocus,Mode=TwoWay,Converter={StaticResource TelephoneConverter}}" Grid.Column="1" VerticalAlignment="Center" GotFocus="RepoLocationTextBox_GotFocus" />
                            </Grid>
                        </Grid>
                    </TabItem>
                    <TabItem>
                        <TabItem.Header>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="&#xE825;" FontFamily="Segoe MDL2 Assets" FontSize="12" VerticalAlignment="Center" Margin="0,0,5,0" />
                                <TextBlock Text="Rates" VerticalAlignment="Center" />
                            </StackPanel>
                        </TabItem.Header>
                        <TabItem.Resources>
                            <Style TargetType="ListBox">
                                <Setter Property="BorderThickness" Value="0" />
                                <Setter Property="Margin" Value="0,5,0,0" />
                                <Setter Property="ScrollViewer.VerticalScrollBarVisibility" Value="Visible" />
                            </Style>                            
                            <Style TargetType="GroupBox">
                                <Setter Property="VerticalContentAlignment" Value="Stretch" />
                                <Setter Property="Height" Value="170" />
                            </Style>
                        </TabItem.Resources>
                        <ScrollViewer VerticalScrollBarVisibility="Hidden" HorizontalScrollBarVisibility="Auto" Padding="0,5">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="auto" />
                                    <RowDefinition />
                                </Grid.RowDefinitions>
                                
                                <StackPanel Orientation="Horizontal" Grid.Row="1">
                                    <GroupBox Header="Minimum Premium">
                                        <ListBox ItemsSource="{Binding LsMinpItems}" ItemContainerStyle="{StaticResource LBItemContainer}">
                                            <ListBox.ItemTemplate>
                                                <DataTemplate>
                                                    <Grid>
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition />
                                                            <RowDefinition />
                                                            <RowDefinition />
                                                            <RowDefinition />
                                                            <RowDefinition />
                                                        </Grid.RowDefinitions>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="Auto" />
                                                            <ColumnDefinition />
                                                            <ColumnDefinition />
                                                            <ColumnDefinition />
                                                            <ColumnDefinition />
                                                            <ColumnDefinition />
                                                        </Grid.ColumnDefinitions>
                                                        <TextBlock Text="{Binding Id,StringFormat=00}" Grid.Row="0" Grid.Column="0" Grid.RowSpan="4" TextAlignment="Right" Margin="0,0,5,0" />
                                                        <TextBlock Text="Male Non-Smoker" Grid.Row="0" Grid.Column="1" VerticalAlignment="Center" Margin="0,0,5,0" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="0" Grid.Column="2" Text="{Binding MaleNonSmokerMinp[0],StringFormat=N2}" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="0" Grid.Column="3" Text="{Binding MaleNonSmokerMinp[1],StringFormat=N2}" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="0" Grid.Column="4" Text="{Binding MaleNonSmokerMinp[2],StringFormat=N2}" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="0" Grid.Column="5" Text="{Binding MaleNonSmokerMinp[3],StringFormat=N2}" />
                                                        <TextBlock Text="Male Smoker" Grid.Row="1" Grid.Column="1" VerticalAlignment="Center" Margin="0,0,5,0" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="1" Grid.Column="2" Text="{Binding MaleSmokerMinp[0],StringFormat=N2}" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="1" Grid.Column="3" Text="{Binding MaleSmokerMinp[1],StringFormat=N2}" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="1" Grid.Column="4" Text="{Binding MaleSmokerMinp[2],StringFormat=N2}" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="1" Grid.Column="5" Text="{Binding MaleSmokerMinp[3],StringFormat=N2}" />
                                                        <TextBlock Text="Female Non-Smoker" Grid.Row="2" Grid.Column="1" VerticalAlignment="Center" Margin="0,0,5,0" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="2" Grid.Column="2" Text="{Binding FemaleNonSmokerMinp[0],StringFormat=N2}" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="2" Grid.Column="3" Text="{Binding FemaleNonSmokerMinp[1],StringFormat=N2}" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="2" Grid.Column="4" Text="{Binding FemaleNonSmokerMinp[2],StringFormat=N2}" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="2" Grid.Column="5" Text="{Binding FemaleNonSmokerMinp[3],StringFormat=N2}" />
                                                        <TextBlock Text="Female Smoker" Grid.Row="3" Grid.Column="1" VerticalAlignment="Center" Margin="0,0,5,0" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="3" Grid.Column="2" Text="{Binding FemaleSmokerMinp[0],StringFormat=N2}" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="3" Grid.Column="3" Text="{Binding FemaleSmokerMinp[1],StringFormat=N2}" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="3" Grid.Column="4" Text="{Binding FemaleSmokerMinp[2],StringFormat=N2}" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="3" Grid.Column="5" Text="{Binding FemaleSmokerMinp[3],StringFormat=N2}" />
                                                        <Line Grid.Row="4" Grid.Column="0" Grid.ColumnSpan="6" X1="0" Y1="5" X2="{Binding RelativeSource={RelativeSource AncestorType=Grid},Path=ActualWidth}" Y2="5" StrokeThickness="1" Stroke="Black" />
                                                    </Grid>
                                                </DataTemplate>
                                            </ListBox.ItemTemplate>
                                        </ListBox>
                                    </GroupBox>
                                    <GroupBox Header="CS080 Values for Guideline">
                                        <ListBox ItemsSource="{Binding LsCso80Items}" ItemContainerStyle="{StaticResource LBItemContainer}">
                                            <ListBox.ItemTemplate>
                                                <DataTemplate>
                                                    <Grid>
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition />
                                                            <RowDefinition />
                                                            <RowDefinition />
                                                            <RowDefinition />
                                                            <RowDefinition />
                                                        </Grid.RowDefinitions>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="Auto" />
                                                            <ColumnDefinition />
                                                            <ColumnDefinition />
                                                        </Grid.ColumnDefinitions>
                                                        <TextBlock Text="{Binding Id,StringFormat=00}" Grid.Row="0" Grid.Column="0" Grid.RowSpan="4" TextAlignment="Right" Margin="0,0,5,0" />
                                                        <TextBlock Text="Male Non-Smoker" Grid.Row="0" Grid.Column="1" VerticalAlignment="Center" Margin="0,0,5,0" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="0" Grid.Column="2" Text="{Binding MaleNonSmoker,StringFormat=N2}" />
                                                        <TextBlock Text="Male Smoker" Grid.Row="1" Grid.Column="1" VerticalAlignment="Center" Margin="0,0,5,0" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="1" Grid.Column="2" Text="{Binding MaleSmoker,StringFormat=N2}" />
                                                        <TextBlock Text="Female Non-Smoker" Grid.Row="2" Grid.Column="1" VerticalAlignment="Center" Margin="0,0,5,0" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="2" Grid.Column="2" Text="{Binding FemaleNonSmoker,StringFormat=N2}" />
                                                        <TextBlock Text="Female Smoker" Grid.Row="3" Grid.Column="1" VerticalAlignment="Center" Margin="0,0,5,0" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="3" Grid.Column="2" Text="{Binding FemaleSmoker,StringFormat=N2}" />
                                                        <Line Grid.Row="4" Grid.Column="0" Grid.ColumnSpan="19" X1="0" Y1="5" X2="{Binding RelativeSource={RelativeSource AncestorType=Grid},Path=ActualWidth}" Y2="5" StrokeThickness="1" Stroke="Black" />
                                                    </Grid>
                                                </DataTemplate>
                                            </ListBox.ItemTemplate>
                                        </ListBox>
                                    </GroupBox>
                                    <GroupBox Header="Principal Insured">
                                        <ListBox ItemsSource="{Binding LsRatePrItems}" ItemContainerStyle="{StaticResource LBItemContainer}">
                                            <ListBox.ItemTemplate>
                                                <DataTemplate>
                                                    <Grid>
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition />
                                                            <RowDefinition />
                                                            <RowDefinition />
                                                            <RowDefinition />
                                                            <RowDefinition />
                                                        </Grid.RowDefinitions>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="Auto" />
                                                            <ColumnDefinition />
                                                            <ColumnDefinition />
                                                            <ColumnDefinition />
                                                            <ColumnDefinition />
                                                            <ColumnDefinition />
                                                        </Grid.ColumnDefinitions>
                                                        <TextBlock Text="{Binding Id,StringFormat=00}" Grid.Row="0" Grid.Column="0" Grid.RowSpan="4" TextAlignment="Right" Margin="0,0,5,0" />
                                                        <TextBlock Text="Male Non-Smoker" Grid.Row="0" Grid.Column="1" VerticalAlignment="Center" Margin="0,0,5,0" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="0" Grid.Column="2" Text="{Binding MaleNonSmoker[0],StringFormat=N2}" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="0" Grid.Column="3" Text="{Binding MaleNonSmoker[1],StringFormat=N2}" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="0" Grid.Column="4" Text="{Binding MaleNonSmoker[2],StringFormat=N2}" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="0" Grid.Column="5" Text="{Binding MaleNonSmoker[3],StringFormat=N2}" />
                                                        <TextBlock Text="Male Smoker" Grid.Row="1" Grid.Column="1" VerticalAlignment="Center" Margin="0,0,5,0" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="1" Grid.Column="2" Text="{Binding MaleSmoker[0],StringFormat=N2}" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="1" Grid.Column="3" Text="{Binding MaleSmoker[1],StringFormat=N2}" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="1" Grid.Column="4" Text="{Binding MaleSmoker[2],StringFormat=N2}" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="1" Grid.Column="5" Text="{Binding MaleSmoker[3],StringFormat=N2}" />
                                                        <TextBlock Text="Female Non-Smoker" Grid.Row="2" Grid.Column="1" VerticalAlignment="Center" Margin="0,0,5,0" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="2" Grid.Column="2" Text="{Binding FemaleNonSmoker[0],StringFormat=N2}" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="2" Grid.Column="3" Text="{Binding FemaleNonSmoker[1],StringFormat=N2}" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="2" Grid.Column="4" Text="{Binding FemaleNonSmoker[2],StringFormat=N2}" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="2" Grid.Column="5" Text="{Binding FemaleNonSmoker[3],StringFormat=N2}" />
                                                        <TextBlock Text="Female Smoker" Grid.Row="3" Grid.Column="1" VerticalAlignment="Center" Margin="0,0,5,0" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="3" Grid.Column="2" Text="{Binding FemaleSmoker[0],StringFormat=N2}" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="3" Grid.Column="3" Text="{Binding FemaleSmoker[1],StringFormat=N2}" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="3" Grid.Column="4" Text="{Binding FemaleSmoker[2],StringFormat=N2}" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="3" Grid.Column="5" Text="{Binding FemaleSmoker[3],StringFormat=N2}" />
                                                        <Line Grid.Row="4" Grid.Column="0" Grid.ColumnSpan="19" X1="0" Y1="5" X2="{Binding RelativeSource={RelativeSource AncestorType=Grid},Path=ActualWidth}" Y2="5" StrokeThickness="1" Stroke="Black" />
                                                    </Grid>
                                                </DataTemplate>
                                            </ListBox.ItemTemplate>
                                        </ListBox>
                                    </GroupBox>
                                    <GroupBox Header="Spouse/Additional Insured">
                                        <ListBox ItemsSource="{Binding LsRateSiItems}" ItemContainerStyle="{StaticResource LBItemContainer}">
                                            <ListBox.ItemTemplate>
                                                <DataTemplate>
                                                    <Grid>
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition />
                                                            <RowDefinition />
                                                            <RowDefinition />
                                                            <RowDefinition />
                                                            <RowDefinition />
                                                        </Grid.RowDefinitions>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="Auto" />
                                                            <ColumnDefinition />
                                                            <ColumnDefinition />
                                                            <ColumnDefinition />
                                                        </Grid.ColumnDefinitions>
                                                        <TextBlock Text="{Binding Id,StringFormat=00}" Grid.Row="0" Grid.Column="0" Grid.RowSpan="4" TextAlignment="Right" Margin="0,0,5,0" />
                                                        <TextBlock Text="Male Non-Smoker" Grid.Row="0" Grid.Column="1" VerticalAlignment="Center" Margin="0,0,5,0" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="0" Grid.Column="2" Text="{Binding MaleNonSmoker[0],StringFormat=N2}" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="0" Grid.Column="3" Text="{Binding MaleSmoker[1],StringFormat=N2}" />
                                                        <TextBlock Text="Male Smoker" Grid.Row="1" Grid.Column="1" VerticalAlignment="Center" Margin="0,0,5,0" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="1" Grid.Column="2" Text="{Binding MaleNonSmoker[0],StringFormat=N2}" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="1" Grid.Column="3" Text="{Binding MaleSmoker[1],StringFormat=N2}" />
                                                        <TextBlock Text="Female Non-Smoker" Grid.Row="2" Grid.Column="1" VerticalAlignment="Center" Margin="0,0,5,0" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="2" Grid.Column="2" Text="{Binding MaleNonSmoker[0],StringFormat=N2}" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="2" Grid.Column="3" Text="{Binding MaleSmoker[1],StringFormat=N2}" />
                                                        <TextBlock Text="Female Smoker" Grid.Row="3" Grid.Column="1" VerticalAlignment="Center" Margin="0,0,5,0" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="3" Grid.Column="2" Text="{Binding MaleNonSmoker[0],StringFormat=N2}" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="3" Grid.Column="3" Text="{Binding MaleSmoker[1],StringFormat=N2}" />
                                                        <Line Grid.Row="4" Grid.Column="0" Grid.ColumnSpan="6" X1="0" Y1="5" X2="{Binding RelativeSource={RelativeSource AncestorType=Grid},Path=ActualWidth}" Y2="5" StrokeThickness="1" Stroke="Black" />
                                                    </Grid>
                                                </DataTemplate>
                                            </ListBox.ItemTemplate>
                                        </ListBox>
                                    </GroupBox>
                                    <GroupBox Header="Substandard Rates">
                                        <ListBox ItemsSource="{Binding LsRateSbItems}" ItemContainerStyle="{StaticResource LBItemContainer}">
                                            <ListBox.ItemTemplate>
                                                <DataTemplate>
                                                    <Grid>
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition />
                                                            <RowDefinition />
                                                            <RowDefinition />
                                                        </Grid.RowDefinitions>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="Auto" />
                                                            <ColumnDefinition />
                                                            <ColumnDefinition />
                                                        </Grid.ColumnDefinitions>
                                                        <TextBlock Text="{Binding Id,StringFormat=00}" Grid.Row="0" Grid.Column="0" Grid.RowSpan="3" TextAlignment="Right" Margin="0,0,5,0" />
                                                        <TextBlock Text="Smoker" Grid.Row="0" Grid.Column="1" VerticalAlignment="Center" Margin="0,0,5,0" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="0" Grid.Column="2" Text="{Binding SmokerSubstandard,StringFormat=N2}" />
                                                        <TextBlock Text="Non-Smoker" Grid.Row="1" Grid.Column="1" VerticalAlignment="Center" Margin="0,0,5,0" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="1" Grid.Column="2" Text="{Binding NonSmokerSubstandard,StringFormat=N2}" />
                                                        <Line Grid.Row="3" Grid.Column="0" Grid.ColumnSpan="3" X1="0" Y1="5" X2="{Binding RelativeSource={RelativeSource AncestorType=Grid},Path=ActualWidth}" Y2="5" StrokeThickness="1" Stroke="Black" />
                                                    </Grid>
                                                </DataTemplate>
                                            </ListBox.ItemTemplate>
                                        </ListBox>
                                    </GroupBox>
                                    <GroupBox Header="Waiver of Premium">
                                        <ListBox ItemsSource="{Binding LsRateWpItems}" ItemContainerStyle="{StaticResource LBItemContainer}">
                                            <ListBox.ItemTemplate>
                                                <DataTemplate>
                                                    <Grid>
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition />
                                                            <RowDefinition />
                                                            <RowDefinition />
                                                        </Grid.RowDefinitions>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="Auto" />
                                                            <ColumnDefinition />
                                                            <ColumnDefinition />
                                                        </Grid.ColumnDefinitions>
                                                        <TextBlock Text="{Binding Id,StringFormat=00}" Grid.Row="0" Grid.Column="0" Grid.RowSpan="3" TextAlignment="Right" Margin="0,0,5,0" />
                                                        <TextBlock Text="Male" Grid.Row="0" Grid.Column="1" VerticalAlignment="Center" Margin="0,0,5,0" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="0" Grid.Column="2" Text="{Binding MaleWPD,StringFormat=N2}" />
                                                        <TextBlock Text="Female" Grid.Row="1" Grid.Column="1" VerticalAlignment="Center" Margin="0,0,5,0" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="1" Grid.Column="2" Text="{Binding FemaleWPD,StringFormat=N2}" />
                                                        <Line Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="3" X1="0" Y1="5" X2="{Binding RelativeSource={RelativeSource AncestorType=Grid},Path=ActualWidth}" Y2="5" StrokeThickness="1" Stroke="Black" />
                                                    </Grid>
                                                </DataTemplate>
                                            </ListBox.ItemTemplate>
                                        </ListBox>
                                    </GroupBox>
                                    <GroupBox Header="Target Premium">
                                        <ListBox ItemsSource="{Binding LsTargItems}" ItemContainerStyle="{StaticResource LBItemContainer}">
                                            <ListBox.ItemTemplate>
                                                <DataTemplate>
                                                    <Grid>
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition />
                                                            <RowDefinition />
                                                            <RowDefinition />
                                                        </Grid.RowDefinitions>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="Auto" />
                                                            <ColumnDefinition />
                                                            <ColumnDefinition />
                                                        </Grid.ColumnDefinitions>
                                                        <TextBlock Text="{Binding Id,StringFormat=00}" Grid.Row="0" Grid.Column="0" Grid.RowSpan="3" TextAlignment="Right" Margin="0,0,5,0" />
                                                        <TextBlock Text="Male" Grid.Row="0" Grid.Column="1" VerticalAlignment="Center" Margin="0,0,5,0" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="0" Grid.Column="2" Text="{Binding MaleTarget,StringFormat=N2}" />
                                                        <TextBlock Text="Female" Grid.Row="1" Grid.Column="1" VerticalAlignment="Center" Margin="0,0,5,0" />
                                                        <TextBlock Style="{StaticResource NumericValue}" Grid.Row="1" Grid.Column="2" Text="{Binding FemaleTarget,StringFormat=N2}" />
                                                        <Line Grid.Row="3" Grid.Column="0" Grid.ColumnSpan="3" X1="0" Y1="5" X2="{Binding RelativeSource={RelativeSource AncestorType=Grid},Path=ActualWidth}" Y2="5" StrokeThickness="1" Stroke="Black" />
                                                    </Grid>
                                                </DataTemplate>
                                            </ListBox.ItemTemplate>
                                        </ListBox>
