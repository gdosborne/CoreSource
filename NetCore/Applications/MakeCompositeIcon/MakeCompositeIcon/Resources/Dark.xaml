<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="MyWindowBackground" Color="#FF000000" />
    <SolidColorBrush x:Key="MyWindowForeground" Color="#FFFFFFFF" />
    <SolidColorBrush x:Key="MyBorderBrush" Color="#FF999999" />
    <SolidColorBrush x:Key="MyWorkArea" Color="#FF121212" />
    <SolidColorBrush x:Key="MyButtonBackground" Color="#FF141414" />
    <SolidColorBrush x:Key="MyButtonForeground" Color="#FFFAFAFA" />

    <SolidColorBrush x:Key="VertLightBrush" Color="#FFF9F9F9" />
    <SolidColorBrush x:Key="ToggleOnBackground" Color="#FFFFFFFF" />
    <SolidColorBrush x:Key="ToggleOnForeground" Color="#FF2222FF" />
    <SolidColorBrush x:Key="ToggleOffBackground" Color="#FFCCCCCC" />
    <SolidColorBrush x:Key="ToggleOffForeground" Color="#FF000000" />

</ResourceDictionary>