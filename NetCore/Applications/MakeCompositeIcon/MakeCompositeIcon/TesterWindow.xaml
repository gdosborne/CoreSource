<Window x:Class="MakeCompositeIcon.TesterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MakeCompositeIcon"
        mc:Ignorable="d"
        Title="TesterWindow" Height="450" Width="800">
    <Grid HorizontalAlignment="Center" VerticalAlignment="Center">
        <Grid Background="#00FFFFFF">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="0.5*" />
                <ColumnDefinition Width="0.5*" />
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="0.5*" />
                <RowDefinition Height="0.5*" />
            </Grid.RowDefinitions>
            <TextBlock FontFamily="Segoe Fluent Icons" FontSize="143.43434343434342" Text="" Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="2" Grid.RowSpan="2" Foreground="#FFA40000" HorizontalAlignment="Center" VerticalAlignment="Center" Padding="10" />
            <TextBlock FontFamily="Segoe Fluent Icons" FontSize="54.545454545454504" Text="" Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="2" Grid.RowSpan="2" Foreground="#FFFEFEFE" HorizontalAlignment="Center" VerticalAlignment="Center" Padding="10" />
        </Grid>
    </Grid>
</Window>
