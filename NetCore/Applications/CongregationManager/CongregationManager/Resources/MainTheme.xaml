<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:expr="clr-namespace:Common.Applicationn.Windows.Expressions;assembly=ApplicationFramework"
                    xmlns:mycontrols="clr-namespace:Controls.Core;assembly=Controls.Core"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <sys:Double x:Key="StandardFontSize">16</sys:Double>

    <expr:Calculator x:Key="XLargeFontSize">
        <expr:Product Operand1="{expr:Value {StaticResource StandardFontSize}}" Operand2="1.5" />
    </expr:Calculator>
    <expr:Calculator x:Key="LargeFontSize">
        <expr:Product Operand1="{expr:Value {StaticResource StandardFontSize}}" Operand2="1.3" />
    </expr:Calculator>
    <expr:Calculator x:Key="SmallFontSize">
        <expr:Product Operand1="{expr:Value {StaticResource StandardFontSize}}" Operand2="0.7" />
    </expr:Calculator>
    <expr:Calculator x:Key="XSmallFontSize">
        <expr:Product Operand1="{expr:Value {StaticResource StandardFontSize}}" Operand2="0.6" />
    </expr:Calculator>
    <expr:Calculator x:Key="XXSmallFontSize">
        <expr:Product Operand1="{expr:Value {StaticResource StandardFontSize}}" Operand2="0.55" />
    </expr:Calculator>
    <expr:Calculator x:Key="XXXSmallFontSize">
        <expr:Product Operand1="{expr:Value {StaticResource StandardFontSize}}" Operand2="0.5" />
    </expr:Calculator>

    <expr:Calculator x:Key="ControlHeight">
        <expr:Product Operand1="{expr:Value {StaticResource StandardFontSize}}" Operand2="1.33" />
    </expr:Calculator>

    <SolidColorBrush x:Key="ControlBackground" Color="#FFBB0000" />
    <SolidColorBrush x:Key="ControlForeground" Color="#FFEEEEEE" />
    <SolidColorBrush x:Key="MouseOverBackground" Color="#FFDF7D7D" />
    <SolidColorBrush x:Key="MouseOverForeground" Color="#FF000000" />
    <SolidColorBrush x:Key="WindowBorderBrush" Color="#FF111111" />
    <SolidColorBrush x:Key="LightBorderBrush" Color="#FF777777" />
    <SolidColorBrush x:Key="WindowBackground" Color="#FFFFFFFF" />
    <SolidColorBrush x:Key="CaptionBackground" Color="#FF009999" />
    <SolidColorBrush x:Key="CaptionForeground" Color="#FFDDDDDD" />
    <SolidColorBrush x:Key="WindowTextForeground" Color="#FF222222" />
    <SolidColorBrush x:Key="ToggleSwitchBackground" Color="#200000FF" />

    <FontFamily x:Key="StandardFont">Calibri</FontFamily>
    <FontFamily x:Key="GenderFontFamily">pack://application:,,,/Fonts/#People</FontFamily>
    <FontFamily x:Key="GlyphFontFamily">Segoe Fluent Icons</FontFamily>

